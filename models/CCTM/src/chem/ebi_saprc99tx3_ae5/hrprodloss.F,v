C***********************************************************************

C RCS file, release, date & time of last delta, author, state, [and locker]
C $Header: /project/work/rep/CCTM/src/chem/ebi_saprc99tx3_ae5/hrprodloss.F,v 1.1.1.1 2008/09/08 15:00:57 sjr Exp $ 

C what(1) key, module and SID; SCCS file; date and time of last delta:
C %W% %P% %G% %U%

      SUBROUTINE HRPRODLOSS

C***********************************************************************
C
C  FUNCTION: To compute the production and loss rates for EBI species
C
C  PRECONDITIONS: For the SAPRC99TX3 family of mechanisms
C
C  RETURN VALUES: None
C
C  KEY SUBROUTINES/FUNCTIONS CALLED: None
C
C  REVISION HISTORY: Created by EBI solver program, May 30, 2008
C
C***********************************************************************
      USE HRDATA

      IMPLICIT NONE


C.....INCLUDES: NONE


C.....ARGUMENTS: NONE


C.....PARAMETERS: NONE


C.....EXTERNAL FUNCTIONS: NONE


C.....SAVED VARIABLES: NONE
      CHARACTER( 16 ), SAVE  ::  PNAME = 'HRPRODLOSS'   ! Program name


C.....LOCAL VARIABLES:

      INTEGER ESP

C***********************************************************************


      PROD( HNO3             ) =   2.000 * RXRAT(  13 )      ! N2O5=2*HNO3
     &                          +          RXRAT(  25 )      ! HO+NO2=HNO3
     &                          +  0.200 * RXRAT(  39 )      ! NO3+HO2=0.2*HNO3+0.8*NO2+...
     &                          +          RXRAT( 129 )      ! HCHO+NO3=HNO3+HO2+CO
     &                          +          RXRAT( 132 )      ! CCHO+NO3=HNO3+CCO_O2
     &                          +          RXRAT( 135 )      ! RCHO+NO3=HNO3+RCO_O2
     &                          +          RXRAT( 148 )      ! GLY+NO3=HNO3+0.63*HO2+...
     &                          +          RXRAT( 151 )      ! MGLY+NO3=HNO3+CO+CCO_O2
     &                          +          RXRAT( 154 )      ! PHEN+NO3=HNO3+BZ_O
     &                          +          RXRAT( 156 )      ! CRES+NO3=HNO3+BZ_O
     &                          +          RXRAT( 157 )      ! NPHE+NO3=HNO3+BZNO2_O
     &                          +          RXRAT( 160 )      ! BALD+NO3=HNO3+BZCO_O2
     &                          +  0.500 * RXRAT( 163 )      ! METHACRO+NO3=0.5*HNO3+...
     &                          +  0.150 * RXRAT( 172 )      ! ISOPROD+NO3=0.15*HNO3+...
      LOSS( HNO3             ) =           RXRAT(  27 )      ! HNO3+HO=NO3
     &                          +          RXRAT(  28 )      ! HNO3=HO+NO2

      PROD( CO               ) =           RXRAT( 123 )      ! HCHO=CO+2*HO2
     &                          +          RXRAT( 124 )      ! HCHO=CO
     &                          +          RXRAT( 125 )      ! HCHO+HO=CO+HO2
     &                          +          RXRAT( 129 )      ! HCHO+NO3=CO+HO2+HNO3
     &                          +          RXRAT( 131 )      ! CCHO=CO+HO2+C_O2
     &                          +  0.034 * RXRAT( 133 )      ! RCHO+HO=0.034*CO+...
     &                          +          RXRAT( 134 )      ! RCHO=CO+RO2_R+CCHO+HO2
     &                          +  2.000 * RXRAT( 145 )      ! GLY=2*CO+2*HO2
     &                          +          RXRAT( 146 )      ! GLY=CO+HCHO
     &                          +  1.260 * RXRAT( 147 )      ! GLY+HO=1.26*CO+0.63*HO2+...
     &                          +  1.260 * RXRAT( 148 )      ! GLY+NO3=1.26*CO+0.63*HO2+...
     &                          +          RXRAT( 149 )      ! MGLY=CO+HO2+CCO_O2
     &                          +          RXRAT( 150 )      ! MGLY+HO=CO+CCO_O2
     &                          +          RXRAT( 151 )      ! MGLY+NO3=CO+HNO3+CCO_O2
     &                          +  0.416 * RXRAT( 161 )      ! METHACRO+HO=0.416*CO+...
     &                          +  0.450 * RXRAT( 162 )      ! METHACRO+O3=0.45*CO+...
     &                          +  0.500 * RXRAT( 163 )      ! METHACRO+NO3=0.5*CO+...
     &                          +  0.670 * RXRAT( 165 )      ! METHACRO=0.67*CO+...
     &                          +  0.475 * RXRAT( 167 )      ! MVK+O3=0.475*CO+0.05*RO2_R+...
     &                          +  0.700 * RXRAT( 169 )      ! MVK=0.7*CO+0.3*C_O2+...
     &                          +  0.336 * RXRAT( 170 )      ! ISOPROD+HO=0.336*CO+...
     &                          +  0.498 * RXRAT( 171 )      ! ISOPROD+O3=0.498*CO+...
     &                          +  0.572 * RXRAT( 172 )      ! ISOPROD+NO3=0.572*CO+...
     &                          +  1.233 * RXRAT( 173 )      ! ISOPROD=1.233*CO+...
     &                          +          RXRAT( 178 )      ! DCB1+HO=CO+RO2_R+RCHO
     &                          +  1.500 * RXRAT( 179 )      ! DCB1+O3=1.5*CO+0.5*HO+...
     &                          +          RXRAT( 181 )      ! DCB2=CO+0.5*CCO_O2+0.5*HO2+...
     &                          +          RXRAT( 183 )      ! DCB3=CO+0.5*CCO_O2+0.5*HO2+...
     &                          +  0.500 * RXRAT( 186 )      ! ETHENE+O3=0.5*CO+0.12*HO2+...
     &                          +  0.491 * RXRAT( 188 )      ! ETHENE+O3P=0.491*CO+...
     &                          +  0.275 * RXRAT( 190 )      ! ISOPRENE+O3=0.275*CO+...
     &                          +  0.157 * RXRAT( 194 )      ! TRP1+O3=0.157*CO+0.033*HO2+...
     &                          +  0.160 * RXRAT( 198 )      ! ALK2+HO=0.16*CO+0.121*HO2+...
     &                          +  0.002 * RXRAT( 200 )      ! ALK4+HO=0.002*CO+...
     &                          +  0.345 * RXRAT( 209 )      ! OLE1+O3=0.345*CO+0.056*HO2+...
     &                          +  0.265 * RXRAT( 213 )      ! OLE2+O3=0.265*CO+0.003*HO2+...
     &                          +  0.012 * RXRAT( 215 )      ! OLE2+O3P=0.012*CO+...
     &                          +  0.167 * RXRAT( 225 )      ! ACROLEIN+HO=0.167*CO+...
     &                          +          RXRAT( 226 )      ! ACROLEIN+O3=CO+0.81*HO2+...
     &                          +  0.031 * RXRAT( 227 )      ! ACROLEIN+NO3=0.031*CO+...
     &                          +  1.182 * RXRAT( 229 )      ! ACROLEIN=1.182*CO+1.01*HO2+...
     &                          +  0.250 * RXRAT( 231 )      ! BUTADIENE13+O3=0.25*CO+...
     &                          +  0.230 * RXRAT( 233 )      ! BUTADIENE13+O3P=0.23*CO+...
      LOSS( CO               ) =           RXRAT(  29 )      ! CO+HO=HO2

      PROD( HO2H             ) =           RXRAT(  37 )      ! HO2+HO2=HO2H
     &                          +          RXRAT(  38 )      ! HO2+HO2=HO2H
      LOSS( HO2H             ) =           RXRAT(  41 )      ! HO2H=2*HO
     &                          +          RXRAT(  42 )      ! HO2H+HO=HO2

      PROD( SO2              ) =   0.0
      LOSS( SO2              ) =           RXRAT(  44 )      ! SO2+HO=HO2+SULF+SULRXN

      PROD( SULF             ) =           RXRAT(  44 )      ! HO+SO2=SULF+HO2+SULRXN
      LOSS( SULF             ) =   0.0

      PROD( SULRXN           ) =           RXRAT(  44 )      ! HO+SO2=SULRXN+SULF+HO2
      LOSS( SULRXN           ) =   0.0

      PROD( C_O2             ) =           RXRAT(  71 )      ! CCO_O2+NO=C_O2+NO2
     &                          +          RXRAT(  73 )      ! CCO_O2+NO3=C_O2+NO2
     &                          +  2.000 * RXRAT(  78 )      ! CCO_O2+CCO_O2=2*C_O2
     &                          +          RXRAT(  88 )      ! RCO_O2+CCO_O2=C_O2+CCHO+RO2_R
     &                          +          RXRAT(  99 )      ! BZCO_O2+CCO_O2=C_O2+BZ_O+R2O2
     &                          +          RXRAT( 111 )      ! MA_RCO3+CCO_O2=C_O2+HCHO+...
     &                          +          RXRAT( 116 )      ! TBU_O=C_O2+ACET
     &                          +          RXRAT( 131 )      ! CCHO=C_O2+HO2+CO
     &                          +          RXRAT( 137 )      ! ACET=C_O2+CCO_O2
     &                          +  0.650 * RXRAT( 141 )      ! COOH+HO=0.65*C_O2+0.35*HO+...
     &                          +  0.300 * RXRAT( 169 )      ! MVK=0.3*C_O2+0.7*CO+...
     &                          +          RXRAT( 184 )      ! HO=C_O2
     &                          +  0.300 * RXRAT( 188 )      ! ETHENE+O3P=0.3*C_O2+...
     &                          +  0.250 * RXRAT( 192 )      ! ISOPRENE+O3P=0.25*C_O2+...
     &                          +  0.011 * RXRAT( 200 )      ! ALK4+HO=0.011*C_O2+...
     &                          +  0.076 * RXRAT( 209 )      ! OLE1+O3=0.076*C_O2+...
     &                          +  0.197 * RXRAT( 213 )      ! OLE2+O3=0.197*C_O2+...
     &                          +  0.030 * RXRAT( 214 )      ! OLE2+NO3=0.03*C_O2+...
     &                          +  0.870 * RXRAT( 217 )      ! CCO_OH+HO=0.87*C_O2+...
     &                          +  0.172 * RXRAT( 229 )      ! ACROLEIN=0.172*C_O2+...
      LOSS( C_O2             ) =           RXRAT(  46 )      ! C_O2+NO=NO2+HCHO+HO2
     &                          +          RXRAT(  47 )      ! C_O2+HO2=COOH
     &                          +          RXRAT(  48 )      ! C_O2+NO3=HCHO+HO2+NO2
     &                          +  2.000 * RXRAT(  49 )      ! C_O2+C_O2=MEOH+HCHO
     &                          +  2.000 * RXRAT(  50 )      ! C_O2+C_O2=2*HCHO+2*HO2
     &                          +          RXRAT(  54 )      ! C_O2+RO2_R=HO2+0.75*HCHO+...
     &                          +          RXRAT(  64 )      ! C_O2+RO2_N=HO2+0.25*MEOH+...
     &                          +          RXRAT(  74 )      ! C_O2+CCO_O2=CCO_OH+HCHO
     &                          +          RXRAT(  84 )      ! C_O2+RCO_O2=RCO_OH+HCHO
     &                          +          RXRAT(  95 )      ! C_O2+BZCO_O2=RCO_OH+HCHO
     &                          +          RXRAT( 107 )      ! C_O2+MA_RCO3=RCO_OH+HCHO

      PROD( HCHO             ) =           RXRAT(  46 )      ! C_O2+NO=HCHO+NO2+HO2
     &                          +          RXRAT(  48 )      ! C_O2+NO3=HCHO+HO2+NO2
     &                          +          RXRAT(  49 )      ! C_O2+C_O2=HCHO+MEOH
     &                          +  2.000 * RXRAT(  50 )      ! C_O2+C_O2=2*HCHO+2*HO2
     &                          +  0.750 * RXRAT(  54 )      ! RO2_R+C_O2=0.75*HCHO+HO2+...
     &                          +  0.750 * RXRAT(  64 )      ! RO2_N+C_O2=0.75*HCHO+...
     &                          +          RXRAT(  74 )      ! CCO_O2+C_O2=HCHO+CCO_OH
     &                          +          RXRAT(  84 )      ! RCO_O2+C_O2=HCHO+RCO_OH
     &                          +          RXRAT(  95 )      ! BZCO_O2+C_O2=HCHO+RCO_OH
     &                          +          RXRAT( 104 )      ! MA_RCO3+NO=HCHO+NO2+CCO_O2
     &                          +          RXRAT( 106 )      ! MA_RCO3+NO3=HCHO+NO2+CCO_O2
     &                          +          RXRAT( 107 )      ! MA_RCO3+C_O2=HCHO+RCO_OH
     &                          +          RXRAT( 111 )      ! MA_RCO3+CCO_O2=HCHO+C_O2+...
     &                          +          RXRAT( 112 )      ! MA_RCO3+RCO_O2=HCHO+CCO_O2+...
     &                          +          RXRAT( 113 )      ! MA_RCO3+BZCO_O2=HCHO+...
     &                          +  2.000 * RXRAT( 114 )      ! MA_RCO3+MA_RCO3=2*HCHO+...
     &                          +          RXRAT( 127 )      ! HOCOO=HCHO+HO2
     &                          +          RXRAT( 136 )      ! ACET+HO=HCHO+CCO_O2+R2O2
     &                          +  0.115 * RXRAT( 138 )      ! MEK+HO=0.115*HCHO+...
     &                          +          RXRAT( 140 )      ! MEOH+HO=HCHO+HO2
     &                          +  0.350 * RXRAT( 141 )      ! COOH+HO=0.35*HCHO+0.35*HO+...
     &                          +          RXRAT( 142 )      ! COOH=HCHO+HO2+HO
     &                          +          RXRAT( 146 )      ! GLY=HCHO+CO
     &                          +  0.084 * RXRAT( 161 )      ! METHACRO+HO=0.084*HCHO+...
     &                          +  0.200 * RXRAT( 162 )      ! METHACRO+O3=0.2*HCHO+...
     &                          +  0.670 * RXRAT( 165 )      ! METHACRO=0.67*HCHO+...
     &                          +  0.300 * RXRAT( 166 )      ! MVK+HO=0.3*HCHO+...
     &                          +  0.100 * RXRAT( 167 )      ! MVK+O3=0.1*HCHO+0.05*RO2_R+...
     &                          +  0.055 * RXRAT( 170 )      ! ISOPROD+HO=0.055*HCHO+...
     &                          +  0.125 * RXRAT( 171 )      ! ISOPROD+O3=0.125*HCHO+...
     &                          +  0.227 * RXRAT( 172 )      ! ISOPROD+NO3=0.227*HCHO+...
     &                          +  0.300 * RXRAT( 173 )      ! ISOPROD=0.3*HCHO+...
     &                          +  0.213 * RXRAT( 174 )      ! PROD2+HO=0.213*HCHO+...
     &                          +  0.506 * RXRAT( 175 )      ! PROD2=0.506*HCHO+...
     &                          +  0.010 * RXRAT( 176 )      ! RNO3+HO=0.01*HCHO+...
     &                          +  0.134 * RXRAT( 177 )      ! RNO3=0.134*HCHO+0.341*HO2+...
     &                          +  1.610 * RXRAT( 185 )      ! ETHENE+HO=1.61*HCHO+RO2_R+...
     &                          +          RXRAT( 186 )      ! ETHENE+O3=HCHO+0.12*HO2+...
     &                          +  0.191 * RXRAT( 188 )      ! ETHENE+O3P=0.191*HCHO+...
     &                          +  0.624 * RXRAT( 189 )      ! ISOPRENE+HO=0.624*HCHO+...
     &                          +  0.592 * RXRAT( 190 )      ! ISOPRENE+O3=0.592*HCHO+...
     &                          +  0.240 * RXRAT( 192 )      ! ISOPRENE+O3P=0.24*HCHO+...
     &                          +  0.276 * RXRAT( 193 )      ! TRP1+HO=0.276*HCHO+...
     &                          +  0.235 * RXRAT( 194 )      ! TRP1+O3=0.235*HCHO+...
     &                          +  0.039 * RXRAT( 198 )      ! ALK2+HO=0.039*HCHO+...
     &                          +  0.026 * RXRAT( 199 )      ! ALK3+HO=0.026*HCHO+...
     &                          +  0.024 * RXRAT( 200 )      ! ALK4+HO=0.024*HCHO+...
     &                          +  0.026 * RXRAT( 201 )      ! ALK5+HO=0.026*HCHO+...
     &                          +  0.732 * RXRAT( 208 )      ! OLE1+HO=0.732*HCHO+...
     &                          +  0.500 * RXRAT( 209 )      ! OLE1+O3=0.5*HCHO+0.056*HO2+...
     &                          +  0.244 * RXRAT( 212 )      ! OLE2+HO=0.244*HCHO+...
     &                          +  0.269 * RXRAT( 213 )      ! OLE2+O3=0.269*HCHO+...
     &                          +  0.079 * RXRAT( 214 )      ! OLE2+NO3=0.079*HCHO+...
     &                          +  0.083 * RXRAT( 225 )      ! ACROLEIN+HO=0.083*HCHO+...
     &                          +  0.500 * RXRAT( 226 )      ! ACROLEIN+O3=0.5*HCHO+...
     &                          +  0.340 * RXRAT( 229 )      ! ACROLEIN=0.34*HCHO+...
     &                          +  0.481 * RXRAT( 230 )      ! BUTADIENE13+HO=0.481*HCHO+...
     &                          +  0.500 * RXRAT( 231 )      ! BUTADIENE13+O3=0.5*HCHO+...
      LOSS( HCHO             ) =           RXRAT( 123 )      ! HCHO=2*HO2+CO
     &                          +          RXRAT( 124 )      ! HCHO=CO
     &                          +          RXRAT( 125 )      ! HCHO+HO=HO2+CO
     &                          +          RXRAT( 126 )      ! HCHO+HO2=HOCOO
     &                          +          RXRAT( 129 )      ! HCHO+NO3=HNO3+HO2+CO

      PROD( COOH             ) =           RXRAT(  47 )      ! C_O2+HO2=COOH
      LOSS( COOH             ) =           RXRAT( 141 )      ! COOH+HO=0.35*HCHO+0.35*HO+...
     &                          +          RXRAT( 142 )      ! COOH=HCHO+HO2+HO

      PROD( MEOH             ) =           RXRAT(  49 )      ! C_O2+C_O2=MEOH+HCHO
     &                          +  0.250 * RXRAT(  54 )      ! RO2_R+C_O2=0.25*MEOH+...
     &                          +  0.250 * RXRAT(  64 )      ! RO2_N+C_O2=0.25*MEOH+HO2+...
      LOSS( MEOH             ) =           RXRAT( 140 )      ! MEOH+HO=HCHO+HO2

      PROD( RO2_R            ) =           RXRAT(  81 )      ! RCO_O2+NO=RO2_R+CCHO+NO2
     &                          +          RXRAT(  83 )      ! RCO_O2+NO3=RO2_R+CCHO+NO2
     &                          +          RXRAT(  88 )      ! RCO_O2+CCO_O2=RO2_R+CCHO+C_O2
     &                          +  2.000 * RXRAT(  89 )      ! RCO_O2+RCO_O2=2*RO2_R+2*CCHO
     &                          +          RXRAT( 100 )      ! BZCO_O2+RCO_O2=RO2_R+CCHO+...
     &                          +          RXRAT( 112 )      ! MA_RCO3+RCO_O2=RO2_R+...
     &                          +  0.034 * RXRAT( 133 )      ! RCHO+HO=0.034*RO2_R+...
     &                          +          RXRAT( 134 )      ! RCHO=RO2_R+CCHO+CO+HO2
     &                          +  0.370 * RXRAT( 138 )      ! MEK+HO=0.37*RO2_R+...
     &                          +          RXRAT( 139 )      ! MEK=RO2_R+CCHO+CCO_O2
     &                          +  0.340 * RXRAT( 143 )      ! ROOH+HO=0.34*RO2_R+RCHO+...
     &                          +  0.760 * RXRAT( 153 )      ! PHEN+HO=0.76*RO2_R+...
     &                          +  0.760 * RXRAT( 155 )      ! CRES+HO=0.76*RO2_R+...
     &                          +  0.500 * RXRAT( 161 )      ! METHACRO+HO=0.5*RO2_R+...
     &                          +  0.100 * RXRAT( 162 )      ! METHACRO+O3=0.1*RO2_R+...
     &                          +  0.500 * RXRAT( 163 )      ! METHACRO+NO3=0.5*RO2_R+...
     &                          +  0.330 * RXRAT( 165 )      ! METHACRO=0.33*RO2_R+...
     &                          +  0.300 * RXRAT( 166 )      ! MVK+HO=0.3*RO2_R+...
     &                          +  0.050 * RXRAT( 167 )      ! MVK+O3=0.05*RO2_R+...
     &                          +  0.670 * RXRAT( 170 )      ! ISOPROD+HO=0.67*RO2_R+...
     &                          +  0.048 * RXRAT( 171 )      ! ISOPROD+O3=0.048*RO2_R+...
     &                          +  0.799 * RXRAT( 172 )      ! ISOPROD+NO3=0.799*RO2_R+...
     &                          +  0.473 * RXRAT( 174 )      ! PROD2+HO=0.473*RO2_R+...
     &                          +  0.960 * RXRAT( 175 )      ! PROD2=0.96*RO2_R+...
     &                          +  0.376 * RXRAT( 176 )      ! RNO3+HO=0.376*RO2_R+...
     &                          +  0.564 * RXRAT( 177 )      ! RNO3=0.564*RO2_R+0.341*HO2+...
     &                          +          RXRAT( 178 )      ! DCB1+HO=RO2_R+RCHO+CO
     &                          +          RXRAT( 181 )      ! DCB2=RO2_R+0.5*CCO_O2+...
     &                          +          RXRAT( 183 )      ! DCB3=RO2_R+0.5*CCO_O2+...
     &                          +          RXRAT( 185 )      ! ETHENE+HO=RO2_R+1.61*HCHO+...
     &                          +          RXRAT( 187 )      ! ETHENE+NO3=RO2_R+RCHO
     &                          +  0.200 * RXRAT( 188 )      ! ETHENE+O3P=0.2*RO2_R+...
     &                          +  0.907 * RXRAT( 189 )      ! ISOPRENE+HO=0.907*RO2_R+...
     &                          +  0.066 * RXRAT( 190 )      ! ISOPRENE+O3=0.066*RO2_R+...
     &                          +  0.749 * RXRAT( 191 )      ! ISOPRENE+NO3=0.749*RO2_R+...
     &                          +  0.750 * RXRAT( 193 )      ! TRP1+HO=0.75*RO2_R+...
     &                          +  0.031 * RXRAT( 194 )      ! TRP1+O3=0.031*RO2_R+...
     &                          +  0.276 * RXRAT( 195 )      ! TRP1+NO3=0.276*RO2_R+...
     &                          +          RXRAT( 197 )      ! ALK1+HO=RO2_R+CCHO
     &                          +  0.612 * RXRAT( 198 )      ! ALK2+HO=0.612*RO2_R+...
     &                          +  0.695 * RXRAT( 199 )      ! ALK3+HO=0.695*RO2_R+...
     &                          +  0.835 * RXRAT( 200 )      ! ALK4+HO=0.835*RO2_R+...
     &                          +  0.653 * RXRAT( 201 )      ! ALK5+HO=0.653*RO2_R+...
