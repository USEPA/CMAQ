         ELSE
            XMSG = 
     &         'Could not find ' // VNAME // 'in gas chem aerosol table'
            CALL M3MESG ( XMSG )
            CALL M3MESG 
     &             ( 'No production of organic aerosols from alkanes' )
            LALKRXN = 0
         END IF

         VNAME = 'XYLNRXN'
         N = INDEX1( VNAME, N_GC_G2AE, GC_G2AE )
         IF ( N .NE. 0 ) THEN
            LXYLNRXN = GC_STRT - 1 + GC_G2AE_MAP( N )
