            DO SPC = 1, 15

               WRITE(VNAME, '("AORGP",i2,"J")') SPC
               IF ( SPC .LE. 9 ) VNAME(6:6) = "0"
               N = INDEX1( VNAME, N_AE_SPC, AE_SPC )
               IF ( N .NE. 0 ) THEN
                  V = V + 1
                  VORGPJIND( SPC ) = V
                  CBLK_MAP( V ) = AE_STRT - 1 + N
               ELSE
                  XMSG = 'Could not find ' // VNAME // 'in aerosol table'
                  CALL M3EXIT( PNAME, JDATE, JTIME, XMSG, XSTAT3 ) 
                  CALL M3MESG ( XMSG )
                  VORGPJIND( SPC ) = 0
               END IF

               WRITE(VNAME, '("AORGP",i2,"I")') SPC
               IF ( SPC .LE. 9 ) VNAME(6:6) = "0"
               N = INDEX1( VNAME, N_AE_SPC, AE_SPC )
